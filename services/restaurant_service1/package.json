{
  "type": "module",
  "name": "restaurant_service",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "pnpm run build:ts && tsc -p test/tsconfig.json && FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --test --experimental-test-coverage --loader ts-node/esm test/**/*.ts",
    "start": "pnpm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc -p tsconfig.json",
    "watch:ts": "tsc -w",
    "dev": "pnpm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "db:generate": "pnpx drizzle-kit generate",
    "db:migrate": "pnpx drizzle-kit migrate",
    "db:seed": "pnpx tsx src/db/seed/seed.ts",
    "lint": "eslint 'src/**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/sensible": "^6.0.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.0.1",
    "fastify-plugin": "^5.0.0",
    "fastify-type-provider-zod": "^4.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "eslint": "^9.14.0",
    "fastify-tsconfig": "^2.0.0",
    "globals": "^15.12.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0"
  }
}